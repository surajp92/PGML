FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          setFieldsDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 273
    volVectorFieldValue U (0 0 0)
);

regions
(
    boxToCell
    {
        box (0.04 0.04 -1) (0.06 0.06 1);

        fieldValues
        (
            volScalarFieldValue T 500
        );
    }
);


